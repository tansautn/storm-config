<application>
  <component name="LogHighlightingSettings">
    <highlightingPatterns>
      <LogHighlightingPattern pattern="(INFO|DEBUG)" fg="-8519700" italic="true" />
      <LogHighlightingPattern pattern="WARNING|NOTICE" fg="-28650" stripe="true" />
      <LogHighlightingPattern pattern="\[ERROR\]" action="HIGHLIGHT_LINE" fg="-50359" bold="true" stripe="true" />
      <LogHighlightingPattern pattern="CRITICAL" action="HIGHLIGHT_LINE" fg="-50359" bold="true" stripe="true" />
    </highlightingPatterns>
    <parsingPatterns>
      <LogParsingPattern name="Monolog One Line" pattern="([0-9-. ]+)\[(\w+)\] \&gt; (.+)" timePattern="y-M-d H-m-s.S" linePattern="^" timeId="0" severityId="1" categoryId="1" />
    </parsingPatterns>
    <settingsVersion>4</settingsVersion>
    <lastAddedDefaultFormat>3</lastAddedDefaultFormat>
    <readonlySizeThreshold>64</readonlySizeThreshold>
  </component>
</application>